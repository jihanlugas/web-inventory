{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "next/core-web-vitals",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/prop-types": [
      "warn",
      {
        "ignore": [
          "dispatch"
        ],
        "skipUndeclared": true
      }
    ],
    "no-extra-semi": "warn",
    "indent": [
      "off"
    ],
    "linebreak-style": 0,
    "quotes": [
      "warn",
      "single"
    ],
    "semi": [
      "warn",
      "always"
    ]
  }
}